data lucro_acumulado;
 set data_lucro;
 lucro_acumulado = sum(lucro,lucro_acumulado);
run;

data lucro_acumulado_retain;
 set data_lucro;
 retain lucro_acumulado;
 lucro_acumulado = sum(lucro,lucro_acumulado);
 format lucro lucro_acumulado comma12.0;
run;  
